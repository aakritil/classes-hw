 RowBox[{
  RowBox[{"overall", "[", 
   RowBox[{"p_", ",", "s_", ",", "t_"}], "]"}], " ", ":=", " ", 
  RowBox[{
   RowBox[{"0.45", " ", "p"}], "+", " ", 
   RowBox[{"0.4", " ", "s"}], " ", "+", " ", 
   RowBox[{"0.15", " ", "t"}]}]}]], "Input",
 CellChangeTimes->{{3.47487324511108*^9, 3.474873373664481*^9}, {
  3.6012921959479837`*^9, 3.6012922241630077`*^9}}],

